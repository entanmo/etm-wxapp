page{background: #fff;}
.binding{margin-top: 50rpx;}
.binding .isbinding{font-size: 30rpx;margin: 0 30rpx;color: #999;}
.binding .binding-form{padding: 0 30rpx}
.binding .binding-box{height: 80rpx;margin:0 30rpx 0 30rpx;color: #666;border-bottom: 1rpx solid #ccc;}
.binding .binding-box .binding-img1{width: 35rpx;height: 30rpx;margin: 22rpx 25rpx 0 0;}
.binding .binding-box .binding-img2{width: 31rpx;height: 35rpx;margin: 20rpx 25rpx 0 0;}
.binding .binding-box .binding-input{width: 590rpx;height: 70rpx;font-size: 30rpx;padding-left:15rpx;}
.binding .binding-hint{font-size: 25rpx;margin: 20rpx 0 0 40rpx;color: #41AEFF;}
.binding button{height: 76rpx;line-height: 76rpx;color: #fff;font-size: 30rpx;background: #41AEFF;margin: 80rpx 50rpx 0 50rpx;border-radius:12rpx;letter-spacing: 5rpx;box-shadow: 0 3rpx 3rpx #41AEFF;}
.binding .register{font-size: 30rpx;margin-top: 50rpx;color: #41AEFF;top: 0;left: 47%;transform: translateX(-50%)}


/* ETM复制弹窗 */
.register-popup{top: 0;left: 0;width: 100%;height: 100%;background: rgba(0,0,0,0.6);z-index: 999;}
.register-box{top: 200rpx;left: 50%;width: 540rpx;height: 450rpx;margin-left: -270rpx;background: #fff;border-radius: 30rpx;overflow: hidden;}
.register-box .register-text1{font-size: 34rpx;margin: 100rpx 0 30rpx 0;color: #555454;}
.register-box .register-text2{font-size: 32rpx;margin: 30rpx 0 30rpx 0;color: #555454;}
.register-box .confirm{width: 480rpx;height: 80rpx;line-height: 80rpx;border-radius: 80rpx;margin: 50rpx 0 0 0;font-size: 30rpx;color: #fffefe;background: #119EF3}
.register-popup .close{top: 670rpx;left: 50%;margin-left: -29rpx;width: 58rpx;height: 58rpx;z-index: 99;}
.register-popup .close .close-img{width: 55rpx;height: 55rpx;}

/* 绑定成功弹窗 */
.succeed-popup{top: 0;left: 0;width: 100%;height: 100%;background: rgba(0,0,0,0.6);z-index: 999;}
.succeed-box{top: 200rpx;left: 50%;width: 450rpx;height: 350rpx;margin-left: -225rpx;background: #fff;border-radius: 25rpx;overflow: hidden;}
.succeed-box .succeed-text{font-size: 34rpx;margin: 100rpx 0 30rpx 0;color: #555454;}
.succeed-box .confirm{width: 360rpx;height: 70rpx;line-height: 70rpx;border-radius: 70rpx;margin: 50rpx 0 0 0;font-size: 30rpx;color: #fffefe;background: #119EF3}
